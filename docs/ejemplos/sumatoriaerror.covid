%virus Sumatoria pLista :{
    molecula pos := 0
    molecula resultado := /*/
    molecula largoLista := @antibiotico pLista
    cuarentena pos #menorke largoLista :{
        resultado := resultado #mas @penicilina pLista, pos
        pos := pos #mas 1
    }eXXmpalido
    @vomito resultado
}empalido